{
  "name": "trimal",
  "version": "1.5.0",
  "description": "A tool for automated alignment trimming in large-scale phylogenetic analyses",
  "homepage": "https://github.com/inab/trimal",
  "license": "GPL-3.0",
  "type": "make",
  "downloads": {
    "source": {
      "url": "https://github.com/inab/trimal/archive/refs/tags/v1.5.0.tar.gz",
      "rename": {
        "trimal-*": "trimal"
      },
      "clean": [
        "trimal/dataset",
        "trimal/docs"
      ]
    }
  },
  "tests": [
    {
      "name": "version",
      "command": "trimal",
      "pattern": "trimAl v[0-9]+\\.",
      "args": "--version"
    }
  ]
}
