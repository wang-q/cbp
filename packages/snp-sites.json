{
  "name": "snp-sites",
  "version": "2.5.1",
  "description": "Finds SNP sites from a multi-FASTA alignment file",
  "homepage": "https://github.com/sanger-pathogens/snp-sites",
  "license": "GPL-3.0",
  "type": "autotools",
  "downloads": {
    "source": {
      "url": "https://github.com/sanger-pathogens/snp-sites/archive/refs/tags/v2.5.1.tar.gz",
      "rename": {
        "snp-sites-*": "snp-sites"
      },
      "clean": [
        "snp-sites/example_data/*.gz"
      ]
    }
  },
  "tests": [
    {
      "name": "version",
      "command": "snp-sites",
      "pattern": "snp-sites [0-9]+\\.",
      "args": "-V"
    }
  ]
}
