{
  "name": "intspan",
  "version": "0.8.6",
  "description": "Command line tools for IntSpan operations",
  "homepage": "https://github.com/wang-q/intspan",
  "license": "MIT",
  "type": "rust",
  "downloads": {
    "linux": {
      "url": "https://github.com/wang-q/intspan/releases/download/v0.8.6/intspan-x86_64-unknown-linux-gnu.tar.gz",
      "extract": "tar xvfz",
      "binary": [
        "spanr",
        "rgr",
        "linkr"
      ]
    },
    "macos": {
      "url": "https://github.com/wang-q/intspan/releases/download/v0.8.6/intspan-x86_64-apple-darwin.tar.gz",
      "extract": "tar xvfz",
      "binary": [
        "spanr",
        "rgr",
        "linkr"
      ]
    },
    "windows": {
      "url": "https://github.com/wang-q/intspan/releases/download/v0.8.6/intspan-x86_64-pc-windows-msvc.zip",
      "extract": "unzip",
      "binary": [
        "spanr.exe",
        "rgr.exe",
        "linkr.exe"
      ]
    }
  },
  "tests": [
    {
      "name": "version",
      "command": "spanr",
      "pattern": "spanr [0-9]+\\.",
      "args": "--version"
    }
  ]
}
