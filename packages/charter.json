{
  "name": "charter",
  "version": "210112",
  "description": "Charter is a serif typeface designed by Matthew Carter",
  "homepage": "https://practicaltypography.com/charter.html",
  "license": "Bitstream Charter",
  "downloads": {
    "font": {
      "url": "https://practicaltypography.com/fonts/Charter%20210112.zip",
      "extract": "unzip -j -o",
      "binary": "*.ttf"
    }
  },
  "tests": [
    {
      "name": "files",
      "command": "ls",
      "pattern": "Charter Regular\\.ttf$",
      "args": "$(cbp prefix)/share/fonts"
    }
  ]
}
